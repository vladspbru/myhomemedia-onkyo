import QmlProject 1.0

Project {
    /* Include .qml, .js, and image files from current directory and subdirectories */
    QmlFiles {
        directory: "onkyoapp/qml/onkyoapp"
    }
    JavaScriptFiles {
        directory: "onkyoapp/qml/onkyoapp/contents"
    }
    ImageFiles {
        directory: "onkyoapp/qmlonkyoapp/contents"
    }
    /* List of plugin directories passed to QML runtime */
    importPaths: [ "onkyo_client" ]
}
